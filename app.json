{
  "expo": {
    "name": "HashTag",
    "slug": "hashtag",
    "scheme": "hashtag",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/resources/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/resources/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hashtag"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/resources/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": ["expo-router"]
  }
}
